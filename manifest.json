{
    "manifest_version": 2,
    "name": "Quotes for daily inspiration",
    "version": "1.0",
    "description": "Get a quote for your daily inspiration",
    "author": "Alex Panko",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": ["storage", "https://zenquotes.io/"],
